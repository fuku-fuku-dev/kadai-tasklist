<ul class="list-unstyled">
  <% tasks.each do |task| %>
    <li class="media mb-3">
      <div class="media-body">
        <div>
          <%= link_to task.id, task %>
        </div>
          <p><%= task.content %> <%= task.status %></p>
      </div>
    </li>
  <% end %>
  <%= paginate tasks %>
</ul>